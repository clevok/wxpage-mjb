.text--center {
    text-align: center;
}
.text--left {
    text-align: left;
}
.text--right {
    text-align: right;
}

.font-weight-thin {
    font-weight: 100 !important;
  }
  .font-weight-light {
    font-weight: 300 !important;
  }
  .font-weight-regular {
    font-weight: 400 !important;
  }
  .font-weight-medium {
    font-weight: 600 !important;
  }
  .font-weight-bold {
    font-weight: 700 !important;
  }
  .font-weight-black {
    font-weight: 900 !important;
  }

.text--smaller {
    font-size: 22rpx;
}
.text--size-1,.text--small {
    font-size: 24rpx;
}
.text--size-2 {
    font-size: 26rpx;
    line-height: 34rpx;
}
.text--size-3,.text--middle {
    font-size: 28rpx;
    line-height: 40rpx;
}
.text--size-4 {
    font-size: 32rpx;
}
.text--size-5 {
    font-size: 38rpx;
}
.text--size-6,.text--large {
    font-size: 48rpx;
    line-height: 70rpx;
}
.text--title {
    font-size: 48rpx;
    font-weight: 600;
    line-height: 70rpx;
}

/** 
 * 针对 ipad 的屏幕
 */
 @media screen and (min-width: 768px) {
    .text--smaller {
        font-size: 11px;
    }
    .text--size-1,.text--small {
        font-size: 12px;
    }
    .text--size-2 {
        font-size: 13px;
    }
    .text--size-3,.text--middle {
        font-size: 14px;
        line-height: 20px;
    }
    .text--size-4 {
        font-size: 16px;
    }
    .text--size-5 {
        font-size: 19px;
    }
    .text--size-6,.text--large {
        font-size: 24px;
        line-height: 37px;
    }
    .text--title {
        font-size: 24px;
        line-height: 35px;
    }
    
}

